programa {
  funcao inicio() {
    inteiro n1, n2
    escreva ("Digite um número: ")
    leia (n1)
    escreva ("Digite outro número: ")
    leia (n2)
    se (n1>n2)
      escreva(n1," é maior que ",n2)
    se (n1<n2)
      escreva(n1," é menor que ",n2)
    se(n1==n2)
      escreva("Os dois números são iguais. ")
  }
}
